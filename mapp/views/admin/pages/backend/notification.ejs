
<div class="alert alert-<%= type %> alert-dismissible" id="message-box">
    <button type="button" class="close" data-dismiss="alert" aria-hidden="true"><i class="fa fa-times-circle"></i></button>
    <strong><i class="icon fas fa-<%= (type == "warning")?"exclamation-circle" :"check" %>"></i><%= message %></strong>
</div>